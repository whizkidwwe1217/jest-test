{
	"name": "jest-test",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "npm run clean && ng build hordeflow-common && ng build hordeflowkit && ng build",
		"build:theme": "npm-run-all clean build theme",
		"test": "jest --config ./jest.config.js --verbose",
		"test:watch": "jest --config ./jest.config.js --verbose --watch",
		"test:ci": "jest --config ./jest.config.js --verbose --runInBand",
		"test:coverage": "jest --config ./jest.config.js --verbose --coverage",
		"test:clearCache": "jest --clearCache",
		"test:nocache": "npm run test:clearCache && npm run test",
		"test:karma": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"theme:compile:light": "sass --source-map --precision=8 ./projects/hordeflowkit/src/lib/themes/clarity-light/clarity-light-theme.scss ./src/assets/themes/clarity-light.css",
		"theme:optimize:light": "csso ./src/assets/themes/clarity-light.css -o ./src/assets/themes/clarity-light.min.css",
		"theme:compile:dark": "sass --source-map --precision=8 ./projects/hordeflowkit/src/lib/themes/clarity-dark/clarity-dark-theme.scss ./src/assets/themes/clarity-dark.css",
		"theme:optimize:dark": "csso ./src/assets/themes/clarity-dark.css -o ./src/assets/themes/clarity-dark.min.css",
		"theme:compile:evergreen": "sass --source-map --precision=8 ./projects/hordeflowkit/src/lib/themes/evergreen/evergreen-theme.scss ./src/assets/themes/evergreen.css",
		"theme:prefix:evergreen": "postcss ./src/assets/themes/evergreen.css -r",
		"theme:optimize:evergreen": "csso ./src/assets/themes/evergreen.css -o ./src/assets/themes/evergreen.min.css",
		"theme:evergreen": "npm-run-all theme:compile:evergreen theme:prefix:evergreen theme:optimize:evergreen",
		"theme:compile:nutanix": "sass --source-map --precision=8 ./projects/hordeflowkit/src/lib/themes/nutanix/nutanix-theme.scss ./src/assets/themes/nutanix.css",
		"theme:prefix:nutanix": "postcss ./src/assets/themes/nutanix.css -r",
		"theme:optimize:nutanix": "csso ./src/assets/themes/nutanix.css -o ./src/assets/themes/nutanix.min.css",
		"theme:nutanix": "npm-run-all theme:compile:nutanix theme:prefix:nutanix theme:optimize:nutanix",
		"theme:prefix": "postcss ./src/assets/themes/clarity-light.css -r; postcss ./src/assets/themes/clarity-dark.css -r postcss ./src/assets/themes/evergreen.css -r ./src/assets/themes/nutanix.css -r",
		"theme:optimize": "npm-run-all theme:optimize:light theme:optimize:dark theme:optimize:evergreen theme:optimize:nutanix",
		"theme": "npm-run-all theme:compile:light theme:compile:dark theme:compile:evergreen theme:compile:nutanix theme:prefix theme:optimize",
		"clean": "shx rm -rf dist",
		"test:format": "prettier --list-different ./**/*.{js,json,md,ts,scss}",
		"test:lint": "tslint -p . -c tslint.json \"src/**/*.ts\"",
		"format:fix": "pretty-quick --staged",
		"lint:fix": "tslint -p . -c tslint.json --fix \"src/**/*.ts\" -e \"**/node_modules/**\"",
		"bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/hordeflow/stats.json"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~8.2.14",
		"@angular/cdk": "^8.2.3",
		"@angular/common": "~8.2.14",
		"@angular/compiler": "~8.2.14",
		"@angular/core": "~8.2.14",
		"@angular/forms": "~8.2.14",
		"@angular/platform-browser": "~8.2.14",
		"@angular/platform-browser-dynamic": "~8.2.14",
		"@angular/router": "~8.2.14",
		"@aspnet/signalr": "^1.1.4",
		"@clr/angular": "^3.0.0-next.7",
		"@clr/core": "^3.0.0-next.7",
		"@clr/icons": "^3.0.0-next.7",
		"@clr/ui": "^3.0.0-next.7",
		"@ngrx/effects": "^8.6.0",
		"@ngrx/entity": "^8.6.0",
		"@ngrx/router-store": "^8.6.0",
		"@ngrx/store": "^8.6.0",
		"@swimlane/ngx-charts": "^12.0.1",
		"@webcomponents/custom-elements": "^1.2.1",
		"date-fns": "^2.9.0",
		"date-fns-timezone": "^0.1.4",
		"lodash": "^4.17.15",
		"ngx-highlightjs": "^2.1.4",
		"ngx-progressbar": "^6.0.1",
		"pdfmake": "^0.1.53",
		"popper.js": "^1.16.0",
		"resize-observer-polyfill": "^1.5.1",
		"rxjs": "~6.5.4",
		"timezone-support": "^2.0.2",
		"tslib": "^1.10.0",
		"xlsx": "^0.14.3",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-builders/jest": "^8.3.2",
		"@angular-devkit/build-angular": "^0.803.23",
		"@angular-devkit/build-ng-packagr": "^0.803.23",
		"@angular/cli": "~8.3.21",
		"@angular/compiler-cli": "~8.2.14",
		"@angular/language-service": "~8.2.14",
		"@babel/plugin-transform-runtime": "^7.8.3",
		"@babel/preset-env": "^7.8.3",
		"@ngneat/spectator": "^4.11.1",
		"@ngrx/schematics": "^8.6.0",
		"@ngrx/store-devtools": "^8.6.0",
		"@types/jest": "^24.0.15",
		"@types/lodash": "^4.14.149",
		"@types/node": "~8.9.4",
		"autoprefixer": "^9.7.4",
		"babel-jest": "^24.9.0",
		"codelyzer": "^5.0.0",
		"cpy-cli": "^3.0.0",
		"cross-var": "^1.1.0",
		"csso-cli": "^3.0.0",
		"del-cli": "^3.0.0",
		"husky": "^4.0.10",
		"jest": "^24.9.0",
		"jest-junit": "^6.4.0",
		"jest-junit-reporter": "^1.1.0",
		"jest-preset-angular": "^8.0.0",
		"ng-packagr": "^5.7.1",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^7.1.0",
		"prettier": "^1.19.1",
		"pretty-quick": "^2.0.1",
		"replace": "^1.1.5",
		"sass": "^1.25.0",
		"shx": "^0.3.2",
		"ts-jest": "^24.3.0",
		"ts-node": "^8.6.2",
		"tsickle": ">=0.29.0",
		"tslint": "~5.15.0",
		"typescript": "~3.5.3",
		"webpack-bundle-analyzer": "^3.6.0"
	}
}
